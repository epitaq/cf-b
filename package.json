{
  "name": "festmap",
  "version": "1.0.0",
  "description": "電通大学園祭向けの位置情報ベースSNSアプリ",
  "main": "server/app.js",
  "scripts": {
    "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "start": "concurrently \"npm run server:start\" \"npm run client:start\"",
    "server:dev": "cd server && npm run dev",
    "server:start": "cd server && npm start",
    "server:install": "cd server && npm install",
    "client:dev": "cd client && npm start",
    "client:start": "cd client && npm start",
    "client:build": "cd client && npm run build",
    "client:install": "cd client && npm install",
    "install:all": "npm run server:install && npm run client:install",
    "build": "npm run client:build",
    "init-db": "cd server && npm run init-db",
    "setup": "npm run install:all && npm run init-db",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "festmap",
    "sns",
    "location",
    "react",
    "nodejs",
    "sqlite",
    "university",
    "festival"
  ],
  "author": "FestMap Team",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/festmap.git"
  },
  "bugs": {
    "url": "https://github.com/your-username/festmap/issues"
  },
  "homepage": "https://github.com/your-username/festmap#readme"
}